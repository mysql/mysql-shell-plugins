                    "group": "inline"
                },
                {
                    "command": "msg.openConnection",
                    "when": "view == msg.connections && viewItem =~ /^connection.*/",
                    "group": "inline"
                },
                {
                    "command": "msg.openConnection",
                    "when": "view == msg.connections && viewItem =~ /^connection.*/",
                    "group": "1_msg@1"
                },
                {
                    "command": "msg.openConnectionNewTab",
                    "when": "view == msg.connections && viewItem =~ /^connection.*/",
                    "group": "1_msg@1"
                },
                {
                    "command": "msg.newSessionUsingConnection",
