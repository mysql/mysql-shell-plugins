                    "group": "2_msg@1"
                },
                {
                    "command": "msg.defaultConnection",
                    "when": "view == msg.connections && viewItem == connectionMySQL",
                    "group": "2_msg@1"
                },
                {
                    "command": "msg.editConnection",
                    "when": "view == msg.connections && viewItem =~ /^connection.*/",
                    "group": "3_msg@1"
                },
                {
                    "command": "msg.duplicateConnection",
