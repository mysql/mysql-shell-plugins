{
    "background": "$editor.background",
    "selection.background": "$editor.selectionBackground",
    "editorZone.foreground": "$editor.foreground",
    "resultZone.foreground": "$editor.foreground",
    "tag.background": "$badge.background",
    "tag.foreground": "$badge.foreground",
    "tooltip.background": "$dropdown.background",
    "tooltip.border": "$dropdown.border",
    "tooltip.foreground": "$dropdown.foreground",
    "editorZone.background": {
        "baseColorKey": "editor.background",
        "colorAdjustment": {
            "dark": {
                "lightness": {"absolute": -4}
            },
            "light": {
                "lightness": {"absolute": -8}
            }
        }
    },
    "editorPrompt.result": "$editorZone.background",
    "list.headerBackground": "$list.hoverBackground",
    "menu.blurredBackground": {
        "baseColorKey": "menu.background",
        "colorAdjustment": {
            "dark": {
                "alpha": {"relative": -0.4}
            },
            "light": {
                "alpha": {"absolute": -0.4}
            }
        }
    },
    "progressBar.track": "$editorZone.background",
    "resultView.headerBackground": {
        "baseColorKey": "editor.background",
        "colorAdjustment": {
            "dark": {
                "lightness": {"absolute": 2}
            },
            "light": {
                "lightness": {"absolute": -4}
            }
        }
    },
    "resultView.headerForeground": {
        "baseColorKey": "editor.foreground",
        "colorAdjustment": {
            "dark": {
                "lightness": {"relative": -0.3}
            },
            "light": {
                "lightness": {"absolute": 30}
            }
        }
    },
    "resultStatus.background": "$resultView.headerBackground",
    "resultStatus.foreground": "$resultView.headerForeground",
    "input.border": "$settings.textInputBorder",
    "window.activeBorder": "$dropdown.border",
    "window.inactiveBorder": "$dropdown.border",
    "window.background": {
        "baseColorKey": "editor.background",
        "colorAdjustment": {
            "dark": {
                "lightness": {"absolute": -2}
            },
            "light": {
                "lightness": {"absolute": -1}
            }
        }
    },
    "window.headerBackground": {
        "baseColorKey": "editor.background",
        "colorAdjustment": {
            "dark": {
                "lightness": {"absolute": -4}
            },
            "light": {
                "lightness": {"absolute": -5}
            }
        }
    },
    "window.footerBackground": "$window.background"
}