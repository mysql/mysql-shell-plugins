{
    "name": "mysql-shell-gui",
    "version": "1.2.1",
    "publisher": "Oracle",
    "dependencies": {
        "ansi-to-react": "6.1.6",
        "antlr4-c3": "2.2.1",
        "antlr4ts": "0.5.0-alpha.4",
        "classnames": "2.3.1",
        "color": "4.2.3",
        "d3": "7.3.0",
        "idb": "6.1.5",
        "keyboard-key": "1.1.0",
        "monaco-editor": "0.30.1",
        "preact": "10.7.1",
        "set-interval-async": "2.0.3",
        "tabulator-tables": "5.4.1",
        "topojson-client": "3.1.0",
        "ws": "8.5.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/preact": "2.0.1",
        "@types/color": "3.0.3",
        "@types/d3": "7.1.0",
        "@types/enzyme": "3.10.12",
        "@types/jest": "27.4.1",
        "@types/jsdom": "16.2.14",
        "@types/lodash": "4.14.181",
        "@types/node": "17.0.23",
        "@types/parse5": "6.0.1",
        "@types/prettier": "2.6.0",
        "@types/resize-observer-browser": "0.1.7",
        "@types/selenium-webdriver": "4.0.18",
        "@types/set-interval-async": "1.0.0",
        "@types/tabulator-tables": "5.4.0",
        "@types/topojson-client": "3.1.1",
        "@types/ws": "8.5.3",
        "@typescript-eslint/eslint-plugin": "5.19.0",
        "@typescript-eslint/eslint-plugin-tslint": "5.19.0",
        "acorn": "8.6.0",
        "antlr4ts-cli": "0.5.0-alpha.4",
        "cheerio": "1.0.0-rc.10",
        "enzyme": "3.11.0",
        "enzyme-adapter-preact-pure": "4.0.1",
        "enzyme-to-json": "3.6.2",
        "eslint": "7.32.0",
        "eslint-config-preact": "1.2.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-react": "7.27.1",
        "fake-indexeddb": "3.1.7",
        "identity-obj-proxy": "3.0.0",
        "jest-canvas-mock": "2.3.1",
        "jest-html-reporter": "3.4.2",
        "monaco-editor-webpack-plugin": "6.0.0",
        "monaco-typescript": "4.10.0",
        "mysql-promise": "5.0.0",
        "oci-bastion": "2.9.0",
        "oci-common": "2.9.0",
        "oci-core": "2.9.0",
        "oci-identity": "2.9.0",
        "oci-loadbalancer": "2.9.0",
        "oci-mysql": "2.9.0",
        "parse5": "6.0.1",
        "raw-loader": "4.0.2",
        "react-app-rewired": "2.1.8",
        "react-error-overlay": "6.0.9",
        "react-scripts": "3.4.4",
        "run-my-sql-file": "1.0.0",
        "selenium-webdriver": "4.1.0",
        "typescript": "3.9.10",
        "typescript-eslint-parser": "22.0.0",
        "worker-loader": "3.0.8"
    },
    "resolutions": {
        "react-error-overlay": "6.0.9"
    },
    "scripts": {
        "launch-shell-web-server": "bash ./scripts/launch-shell-webserver.sh",
        "launch-dev-server": "bash ./scripts/launch-dev-server.sh 3001",
        "launch-dev-server-win": "set PORT=3001&&set HTTPS=true&&call scripts/launch-dev-server.cmd",
        "build": "bash ./scripts/production-build.sh false",
        "build-with-maps": "bash ./scripts/production-build.sh true",
        "build-win": "call scripts/production-build.cmd false",
        "build-win-with-maps": "call scripts/production-build.cmd true",
        "lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "update-snapshots": "react-scripts test --runInBand --testMatch [ \"**/tests/unit-tests/**/*.spec.[jt]s?(x)\" ] --setupFilesAfterEnv \"./src/tests/setupUnitTests.ts\" --no-coverage  --watchAll=false -u",
        "unit-tests": "react-scripts test --runInBand --testMatch [ \"**/tests/unit-tests/**/*.spec.[jt]s?(x)\" ] --setupFilesAfterEnv \"./src/tests/setupUnitTests.ts\" --coverage --watchAll=false",
        "e2e-tests-run": "react-scripts test --testMatch [ \"**/tests/e2e/**/*.spec.ts?(x)\" ] --setupFilesAfterEnv \"./src/tests/e2e/setupTests.ts\" --testResultsProcessor \"./node_modules/jest-html-reporter\" --maxWorkers=50% --watchAll=false",
        "e2e-tests-report": "tsc src/tests/e2e/postActions.ts && node src/tests/e2e/postActions.js"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/tests/**",
            "!**/node_modules/**",
            "!src/app-wrapper/**",
            "!src/assets/**",
            "!src/parsing/mysql/generated/**",
            "!src/parsing/SQLite/generated/**",
            "!src/parsing/python/generated/**",
            "!src/**/*.d.ts"
        ],
        "coverageReporters": [
            "json",
            "text",
            "clover",
            "html"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 40,
                "branch": 31,
                "functions": 43,
                "lines": 40
            }
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!monaco-editor)/",
            "/node_modules/(?!tabulator-tables)/"
        ],
        "moduleNameMapper": {
            "^monaco-editor$": "monaco-editor/esm/vs/editor/editor.api",
            "^tabulator-tables$": "tabulator-tables/dist/js/tabulator_esm.js",
            "^.+\\.worker$": "<rootDir>/src/tests/unit-tests/__mocks__/workerMock.ts",
            "^.+runtime\\.d\\.ts$": "<rootDir>/src/tests/unit-tests/__mocks__/typingsMock.ts",
            "^react$": "preact/compat",
            "^react-dom/test-utils$": "preact/test-utils",
            "^react-dom$": "preact/compat",
            "^react/jsx-runtime$": "preact/jsx-runtime"
        }
    }
}
