{
    "name": "mysql-shell-gui",
    "version": "1.19.15",
    "publisher": "Oracle",
    "type": "module",
    "scripts": {
        "run-shell:single-user": "bash ./scripts/launch-shell-webserver.sh 1234",
        "run-shell:single-server": "bash ./scripts/launch-shell-webserver.sh localhost:3306",
        "run-win-shell:web-server": "call ./scripts/launch-shell-webserver.cmd",
        "run-vite:dev-server": "vite",
        "build": "bash ./scripts/production-build.sh",
        "build-with-maps": "bash ./scripts/production-build.sh true",
        "build-win": "pwsh scripts/production-build.ps1 && tsc -p tsconfig.json",
        "build-win-with-maps": "pwsh scripts/production-build.ps1 true && tsc -p tsconfig.json",
        "lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" --fix --ignore-pattern 'src/tests/e2e/**'",
        "unit-tests": "npm run unit-tests:generic && npm run unit-tests:mrs && npm run coverage:report",
        "unit-tests:debug": "node --max_old_space_size=8192 node_modules/.bin/jest -c=unit-tests.jest.config.ts --no-coverage",
        "unit-tests:mrs-debug": "node --max_old_space_size=81292 node_modules/.bin/jest -c=unit-tests-mrs.jest.config.ts",
        "unit-tests:run": "node --no-compilation-cache --expose-gc --max_old_space_size=81292 node_modules/.bin/jest --logHeapUsage",
        "unit-tests:generic": "npm run unit-tests:run -- -c=unit-tests.jest.config.ts --watchAll=false",
        "unit-tests:mrs": "npm run unit-tests:run -- -c=unit-tests-mrs.jest.config.ts --watchAll=false --runInBand",
        "coverage:report": "istanbul-merge --out coverage/coverage-final.json coverage/coverage-generic/coverage-final.json coverage/coverage-mrs/coverage-final.json && nyc report",
        "update-snapshots:generic": "clear && node --no-compilation-cache --expose-gc --max_old_space_size=81292 node_modules/.bin/jest -c=unit-tests.jest.config.ts --no-coverage  --watchAll=false -u",
        "update-snapshots:mrs": "clear && node --no-compilation-cache --expose-gc --max_old_space_size=81292 node_modules/.bin/jest -c=unit-tests-mrs.jest.config.ts --no-coverage  --watchAll=false -u  --runInBand",
        "clear:node-modules": "rm -rf node_modules && rm -f package-lock.json && rm -rf build && rm -rf src/oci-typings",
        "e2e-tests-setup-and-run": "mysqlsh --py -f src/tests/e2e/setup_local_env_and_run.py",
        "e2e-tests-run": "jest -c e2e.jest.config.ts",
        "e2e-tests-report": "node src/tests/e2e/postActions.mjs"
    },
    "dependencies": {
        "anser": "2.3.2",
        "antlr4-c3": "3.4.4",
        "antlr4ng": "3.0.16",
        "classnames": "2.5.1",
        "color": "5.0.0",
        "d3": "7.9.0",
        "idb": "8.0.3",
        "monaco-editor": "0.52.2",
        "pixi-viewport": "6.0.3",
        "pixi.js": "8.11.0",
        "preact": "10.26.9",
        "set-interval-async": "3.0.3",
        "tabulator-tables": "6.3.1",
        "ws": "8.18.3"
    },
    "devDependencies": {
        "@esbuild-plugins/node-globals-polyfill": "0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "0.2.2",
        "@glen/jest-raw-loader": "2.0.0",
        "@preact/preset-vite": "2.10.1",
        "@stylistic/eslint-plugin": "5.0.0",
        "@swc/core": "1.12.5",
        "@swc/jest": "0.2.36",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/preact": "3.2.4",
        "@types/d3": "7.4.3",
        "@types/enzyme": "3.10.18",
        "@types/jest": "30.0.0",
        "@types/jsdom": "21.1.7",
        "@types/node": "22.16.3",
        "@types/resize-observer-browser": "0.1.11",
        "@types/selenium-webdriver": "4.1.19",
        "@types/tabulator-tables": "6.2.6",
        "@types/ws": "8.18.1",
        "@typescript-eslint/eslint-plugin": "8.36.0",
        "@typescript-eslint/parser": "8.36.0",
        "antlr-ng": "1.0.10",
        "autoprefixer": "10.4.21",
        "cheerio": "1.0.0-rc.12",
        "enzyme": "3.11.0",
        "enzyme-adapter-preact-pure": "4.1.0",
        "enzyme-to-json": "3.6.2",
        "eslint": "9.30.1",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jsdoc": "51.3.4",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "fake-indexeddb": "6.0.1",
        "identity-obj-proxy": "3.0.0",
        "istanbul-merge": "2.0.0",
        "jest": "29.7.0",
        "jest-canvas-mock": "2.5.2",
        "jest-environment-jsdom": "30.0.4",
        "jest-html-reporter": "3.10.2",
        "monaco-typescript": "4.10.0",
        "mysql-promise": "5.0.0",
        "nyc": "17.1.0",
        "oci-bastion": "2.111.2",
        "oci-common": "2.111.2",
        "oci-core": "2.111.2",
        "oci-identity": "2.111.2",
        "oci-loadbalancer": "2.111.2",
        "oci-mysql": "2.111.2",
        "oci-objectstorage": "2.111.2",
        "postcss": "8.5.6",
        "postcss-nesting": "13.0.2",
        "rollup-plugin-polyfill-node": "0.13.0",
        "run-my-sql-file": "1.0.0",
        "selenium-webdriver": "4.16.0",
        "terser": "5.43.1",
        "ts-node": "10.9.2",
        "typescript": "5.8.3",
        "typescript-eslint": "8.34.1",
        "vite": "6.3.5",
        "vite-plugin-monaco-editor": "1.1.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}