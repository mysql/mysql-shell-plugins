{
    "name": "mysql-shell-gui",
    "version": "1.18.0",
    "publisher": "Oracle",
    "type": "module",
    "scripts": {
        "launch-shell-web-server": "bash ./scripts/launch-shell-webserver.sh",
        "launch-shell-web-server-win": "call ./scripts/launch-shell-webserver.cmd",
        "launch-dev-server": "vite",
        "build": "bash ./scripts/production-build.sh",
        "build-with-maps": "bash ./scripts/production-build.sh true",
        "build-win": "pwsh scripts/production-build.ps1",
        "lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "unit-tests:debug": "node --max_old_space_size=4096 node_modules/.bin/jest -c=unit-tests.jest.config.ts",
        "unit-tests:run": "node --no-compilation-cache --expose-gc --max_old_space_size=4096 node_modules/.bin/jest --logHeapUsage",
        "unit-tests:generic": "npm run unit-tests:run -- -c=unit-tests.jest.config.ts --watchAll=false",
        "unit-tests:mrs": "npm run unit-tests:run -- -c=unit-tests-mrs.jest.config.ts --watchAll=false --runInBand",
        "coverage:report": "istanbul-merge --out coverage/coverage-final.json coverage/coverage-generic/coverage-final.json coverage/coverage-mrs/coverage-final.json && nyc report",
        "unit-tests": "npm run unit-tests:generic && npm run unit-tests:mrs && npm run coverage:report",
        "update-snapshots": "clear && node --no-compilation-cache --expose-gc --max_old_space_size=4096 node_modules/.bin/jest -c=unit-tests.jest.config.ts --no-coverage  --watchAll=false -u",
        "e2e-tests-setup-and-run": "mysqlsh --py -f src/tests/e2e/setup_local_env_and_run.py",
        "e2e-tests-run": "jest -c e2e.jest.config.ts",
        "e2e-tests-report": "node src/tests/e2e/postActions.mjs"
    },
    "dependencies": {
        "anser": "2.1.1",
        "antlr4-c3": "3.4.1",
        "antlr4ng": "3.0.3",
        "classnames": "2.5.1",
        "color": "4.2.3",
        "d3": "7.8.4",
        "idb": "8.0.0",
        "monaco-editor": "0.49.0",
        "preact": "10.19.3",
        "set-interval-async": "3.0.3",
        "tabulator-tables": "6.2.1",
        "ws": "8.16.0"
    },
    "devDependencies": {
        "@esbuild-plugins/node-globals-polyfill": "0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "0.2.2",
        "@glen/jest-raw-loader": "2.0.0",
        "@preact/preset-vite": "2.8.2",
        "@swc/core": "1.5.7",
        "@swc/jest": "0.2.36",
        "@testing-library/jest-dom": "6.4.5",
        "@testing-library/preact": "3.2.3",
        "@types/color": "3.0.6",
        "@types/d3": "7.4.3",
        "@types/enzyme": "3.10.18",
        "@types/jest": "29.5.12",
        "@types/jsdom": "21.1.6",
        "@types/node": "20.12.12",
        "@types/resize-observer-browser": "0.1.11",
        "@types/selenium-webdriver": "4.1.19",
        "@types/set-interval-async": "1.0.3",
        "@types/tabulator-tables": "6.2.0",
        "@types/ws": "8.5.10",
        "@typescript-eslint/eslint-plugin": "7.11.0",
        "@typescript-eslint/parser": "7.11.0",
        "antlr4ng-cli": "2.0.0",
        "autoprefixer": "10.4.19",
        "enzyme": "3.11.0",
        "enzyme-adapter-preact-pure": "4.1.0",
        "enzyme-to-json": "3.6.2",
        "eslint": "8.56.0",
        "eslint-config-preact": "1.4.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsdoc": "48.2.6",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "fake-indexeddb": "6.0.0",
        "identity-obj-proxy": "3.0.0",
        "istanbul-merge": "2.0.0",
        "nyc": "17.1.0",
        "jest": "29.7.0",
        "jest-canvas-mock": "2.5.2",
        "jest-environment-jsdom": "29.7.0",
        "jest-html-reporter": "3.10.2",
        "monaco-typescript": "4.10.0",
        "mysql-promise": "5.0.0",
        "oci-bastion": "2.86.3",
        "oci-common": "2.86.3",
        "oci-core": "2.86.3",
        "oci-identity": "2.86.3",
        "oci-loadbalancer": "2.86.3",
        "oci-mysql": "2.86.3",
        "oci-objectstorage": "2.86.3",
        "postcss": "8.4.38",
        "postcss-nesting": "12.1.5",
        "rollup-plugin-polyfill-node": "0.13.0",
        "run-my-sql-file": "1.0.0",
        "selenium-webdriver": "4.16.0",
        "terser": "5.31.0",
        "ts-node": "10.9.2",
        "typescript": "5.4.5",
        "vite": "5.2.11",
        "vite-plugin-monaco-editor": "1.1.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "overrides": {
        "cheerio": "1.0.0-rc.3"
    }
}
