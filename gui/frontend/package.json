{
    "name": "mysql-shell-gui",
    "version": "1.15.0",
    "publisher": "Oracle",
    "type": "module",
    "scripts": {
        "launch-shell-web-server": "bash ./scripts/launch-shell-webserver.sh",
        "launch-shell-web-server-win": "call ./scripts/launch-shell-webserver.cmd",
        "launch-dev-server": "vite",
        "build": "bash ./scripts/production-build.sh",
        "build-with-maps": "bash ./scripts/production-build.sh true",
        "build-win": "pwsh scripts/production-build.ps1",
        "lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "unit-tests": "node --no-compilation-cache --expose-gc --max_old_space_size=4096 node_modules/.bin/jest --logHeapUsage -c=unit-tests.jest.config.ts --watchAll=false",
        "update-snapshots": "node --no-compilation-cache --expose-gc --max_old_space_size=4096 node_modules/.bin/jest -c=unit-tests.jest.config.ts --no-coverage  --watchAll=false -u",
        "e2e-tests-setup-and-run": "mysqlsh --py -f src/tests/e2e/setup_local_env_and_run.py",
        "e2e-tests-run": "jest -c e2e.jest.config.ts",
        "e2e-tests-report": "node src/tests/e2e/postActions.mjs"
    },
    "dependencies": {
        "anser": "2.1.1",
