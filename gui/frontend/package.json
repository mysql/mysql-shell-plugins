        "@swc/jest": "0.2.29",
        "@testing-library/jest-dom": "6.1.4",
        "@testing-library/preact": "3.2.3",
        "@types/color": "3.0.5",
        "@types/d3": "7.4.2",
        "@types/enzyme": "3.10.15",
