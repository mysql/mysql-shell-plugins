{
    "name": "mrs-shell-plugin",
    "version": "1.0.0",
    "description": "MySQL REST Service plugin for MySQL Shell",
    "scripts": {
        "update-mrs-parser": "scripts/generate_mrs_parser.sh",
        "fix-rrd-svg-files": "scripts/fix_rrd_svg_files.sh",
        "update-grammar-docs": "mysqlsh -f scripts/update_grammar_docs.py",
        "update-html-docs": "scripts/generate_html_docs.sh",
        "run-grammar-test": "clear; scripts/run_grammar_test.sh",
        "run-md-upgrade-test": "clear; scripts/run_md_upgrade_test.sh",
        "prepare-static-content": "scripts/prepare_default_static_content.sh",
        "run-pytest": "clear; mysqlsh --log-level=debug3 --verbose=4 --py -f run_tests.py #-k test_auth_app_grant_options"
    },
    "publisher": "Oracle",
    "devDependencies": {
        "antlr4ng-cli": "1.0.2"
    }
}
